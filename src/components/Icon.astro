    reactColor: ReactColor,
    nodeColor: NodeColor,
    x: X,
    linkedin: LinkedIn,
    github: GitHub,
    ness: Ness,
    vive: Vive,
}

const IconComponent = iconMap[Astro.props.name]
---

<IconComponent />